<div class="jumbotron">

</div>
<div class="center">
<p id="notice"><%= notice %></p>
<div class="row">
  <% @listings.each do |listing| %>
   <div class="col-md-3">

   <div class="thumbnail">
        <%= link_to image_tag(listing.image.url), listing %>
        
        <div class="caption">
        <h3><%= listing.name %></h3>
        
         <%= listing.description %><br>
      </div>
    </div>
    
     
  </div>
  <% end %>
</div>
</div>

<br>

